  }, {
    "name": "product_name",
    "attribute": "innerText",
    "path": "[itemscope][itemtype='http://schema.org/Product'] h1[itemprop='name']"
  }, {
    "name": "product_url",
    "attribute": "content",
    "path": "[itemscope][itemtype='http://schema.org/Product'] [itemprop='url']:first"
  }, {
    "name": "product_image_url",
    "attribute": "content",
    "path": "[itemscope][itemtype='http://schema.org/Product'] meta[itemprop='image']"
  }, {
    "name": "product_brand_name",
    "attribute": "content",
    "path": "[itemscope][itemtype='http://schema.org/Product'] [itemscope][itemtype='http://schema.org/Brand'] meta[itemprop='name']:first"
  }, {
    "name": "product_brand_url",
    "attribute": "href",
    "path": "[itemscope][itemtype='http://schema.org/Product'] [itemscope][itemtype='http://schema.org/Brand'] a[itemprop='url']:first"
  }, {
    "name": "product_offer",
    "attribute": "content",
    "path": "[itemscope][itemtype='http://schema.org/Product'] [itemscope][itemtype='http://schema.org/Offer'] meta[itemprop='price']:first"
  }, {
    "name": "product_offer_currency",
    "attribute": "content",
